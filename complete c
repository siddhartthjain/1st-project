#include<stdio.h>

int main(){
int a,b,c;
printf(" enter operator ('+','*','-','/')");
char d;
scanf("%c",&d);
printf("enter");

scanf("%d",&a);
printf("enter 2");

scanf("%d",&b);



switch(d)

{
case '+':
c = a+b;
printf("%d",c);
break;
case  '*':
c=a*b;
printf("%d",c);
break;
case '-':
c=a-b;
printf("%d",c);break;
case  '/':
c=a/b;
printf("%d",c);break;
default:
printf("wrong");
}




}